            {
                "id": "helm",
                "aliases": [
                    "helm-template",
                    "helm"
                ],
                "filenamePatterns": [
                    "values.yaml",
                    "Chart.yaml",
                    "requirements.yaml",
                    "**/templates/*.yaml",
                    "**/templates/*.tpl",
                    "**/templates/**/*.yaml",
                    "**/templates/**/*.tpl"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "kubernetes-output",
                "mimetypes": [
                    "text/x-code-output"
                ]
            }
        ],
        "grammars": [
            {
                "language": "helm",
                "scopeName": "source.helm",
                "path": "./syntaxes/helm.tmLanguage.json"
            },
